import java.util.*;
class inversion_Count{
public static void main(String[] args){
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int count=0;
int a[]=new int[n];
for(int i=0;i<n;i++){
a[i]=s.nextInt();
}
for(int i=0;i<n-1;i++){
for(int j=i+1;j<n;j++){
if(a[i]>a[j]){
count++;
}
}
}
System.out.print(count);
}
}
