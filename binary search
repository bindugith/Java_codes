import java.util.*;
class Sort1
{
public static void main(String[] args){
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int a[]=new int[n];
for(int i=0;i<n;i++){
a[i]=s.nextInt();
}
int sno=s.nextInt();
Arrays.sort(a);
for(int i=0;i<n;i++){
System.out.print(a[i]+" ");
}
int r=Arrays.binarySearch(a,sno);
System.out.println(r);
}
}
