import java.util.*;
class missing_Position{
public static void main(String[] args){
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int a[]=new int[n];
for(int i=0;i<n;i++){
a[i]=s.nextInt();
}
Arrays.sort(a);
for(int i=1;i<n;i++){
if(a[i]>0){
if((a[i]+1)!=a[i+1]){
System.out.println(a[i]+1);
break;
}
}
}
}
}
