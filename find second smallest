class second_Smallest{
public static void main(String[] args){
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int a[]=new int[n];
for(int i=0;i<n;i++){
a[i]=s.nextInt();
}
int smallest=a[0];
int second_smallest=a[0];
for(int i=0;i<n;i++){
if(a[i]<smallest){
second_smallest=smallest;
smallest=a[i];
}
else
if(second_smallest>a[i]){
second_smallest=a[i];
}
}
System.out.print(second_smallest);
}
}
